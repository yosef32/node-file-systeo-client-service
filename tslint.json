{
    "defaultSeverity": "error",
    "extends": "tslint-config-airbnb",
    "jsRules": {},
    "rules": {
        "no-increment-decrement": false,
        "import-name": false,
        "max-line-length": false,
        "variable-name": [
            true,
            "ban-keywords",
            "check-format",
            "allow-snake-case",
            "allow-leading-underscore",
            "allow-pascal-case"
        ],
        "ter-indent": [
            true,
            4
        ],
        "ter-prefer-arrow-callback": false
    },
    "rulesDirectory": []
}